# Common variables
NODE_ENV=development

# Supabase variables - used by the monorepo
# IMPORTANT: These variables should be defined ONLY in the root .env files
# Individual packages should import from @monorepo/shared
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# For deployment
SUPABASE_PROJECT_REF=your_project_ref